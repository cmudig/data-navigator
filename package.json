{
    "private": true,
    "workspaces": [
        "packages/data-navigator",
        "packages/inspector",
        "packages/skeleton",
        "packages/bokeh-wrapper",
        "docs"
    ],
    "scripts": {
        "build": "yarn workspace data-navigator build",
        "build:all": "yarn build && yarn workspace @data-navigator/bokeh-wrapper build && yarn docs:build && yarn workspace @data-navigator/inspector build:docs && yarn bokeh-wrapper:build:docs",
        "bokeh-wrapper:build": "yarn workspace @data-navigator/bokeh-wrapper build",
        "clean": "yarn workspace data-navigator clean",
        "server": "python -m http.server",
        "prettier-all-check": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --debug-check \"**/*.{js,jsx,ts,tsx,html,jsx,json,css,scss,md}\"",
        "prettier-all": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"**/*.{js,jsx,ts,tsx,html,jsx,json,css,scss,md}\"",
        "docs:dev": "yarn workspace data-navigator-docs dev",
        "docs:build": "yarn workspace data-navigator-docs build",
        "docs:preview": "yarn workspace data-navigator-docs preview",
        "inspector:dev": "yarn workspace @data-navigator/inspector dev",
        "bokeh-wrapper:dev": "yarn workspace @data-navigator/bokeh-wrapper dev",
        "bokeh-wrapper:build:docs": "yarn workspace @data-navigator/bokeh-wrapper build:docs"
    },
    "devDependencies": {
        "prettier": "^3.0.0"
    }
}
